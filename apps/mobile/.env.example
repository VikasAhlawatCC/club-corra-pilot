# Club Corra Mobile App Environment Configuration

# API Configuration - CHANGE THIS LINE
EXPO_PUBLIC_API_BASE_URL=http://192.168.1.5:3001

# OAuth Configuration
EXPO_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id_here
EXPO_PUBLIC_GOOGLE_REDIRECT_URI=clubcorra://auth/callback

# CDN Configuration
EXPO_PUBLIC_CDN_URL=https://cdn.clubcorra.com

# WebSocket Configuration - CHANGE THIS LINE TOO
EXPO_PUBLIC_WS_URL=http://192.168.1.5:3001

# Sentry Configuration (optional)
EXPO_PUBLIC_SENTRY_DSN=your_sentry_dsn_here

# Analytics Configuration (optional)
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=false

# Environment
EXPO_PUBLIC_ENVIRONMENT=development
EXPO_PUBLIC_DEBUG_MODE=true