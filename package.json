{
  "name": "club-corra-pilot",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/VikasAhlawatCC/club-corra-pilot.git",
  "author": "Vikas <vickyahlawat2019@gmail.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "test:unit": "turbo run test:unit",
    "test:integration": "turbo run test:integration",
    "test:performance": "turbo run test:performance",
    "test:security": "turbo run test:security",
    "test:realtime": "turbo run test:realtime",
    "test:all": "turbo run test:all",
    "test:ci": "turbo run test:ci",
    "test:coverage": "turbo run test:coverage",
    "test:runner": "turbo run test:runner",
    "test:phase3": "ts-node scripts/run-phase3-tests.ts",
    "test:auth-integration": "yarn test:phase3",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "^5.0.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.2",
    "turbo": "^1.10.16",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=16.14.0",
    "yarn": ">=1.22.0"
  }
}
